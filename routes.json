{
  "routes": [
    {
      "name": "Home",
      "path": "/",
      "api": false,
      "get": true
    },
    {
      "name": "AuthOIDCProvider",
      "path": "/auth/provider/:provider",
      "api": false,
      "get": true
    },
    {
      "name": "Auth",
      "path": "/auth",
      "api": true,
      "get": false
    },
    {
      "name": "AuthFlow",
      "path": "/auth/:id",
      "api": true,
      "get": true
    },
    {
      "name": "OIDCAuthorize",
      "path": "/auth/oidc/authorize",
      "api": false,
      "get": true
    },
    {
      "name": "OIDCToken",
      "path": "/auth/oidc/token",
      "api": true,
      "get": false
    },
    {
      "name": "OIDCRevoke",
      "path": "/auth/oidc/revoke",
      "api": true,
      "get": false,
      "apiCors": {
        "allowedOrigins": ["*"],
        "allowedMethods": ["POST"],
        "allowedHeaders": ["Authorization"],
        "maxAge": 600
      }
    },
    {
      "name": "OIDCIntrospect",
      "path": "/auth/oidc/introspect",
      "api": true,
      "get": false
    },
    {
      "name": "OIDCUserInfo",
      "path": "/auth/oidc/userinfo",
      "api": true,
      "get": false
    },
    {
      "name": "OIDCKeys",
      "path": "/.well-known/jwks.json",
      "api": false,
      "get": true,
      "getCors": {
        "allowedOrigins": ["*"],
        "allowedMethods": ["GET", "HEAD"],
        "maxAge": 600
      }
    },
    {
      "name": "OIDCConfiguration1",
      "path": "/.well-known/oauth-authorization-server",
      "api": false,
      "get": true
    },
    {
      "name": "OIDCConfiguration2",
      "path": "/.well-known/openid-configuration",
      "api": false,
      "get": true
    },
    {
      "name": "Applications",
      "path": "/a",
      "api": true,
      "get": true
    },
    {
      "name": "ApplicationCreate",
      "path": "/a/create",
      "api": true,
      "get": true
    },
    {
      "name": "Application",
      "path": "/a/:id",
      "api": true,
      "get": true
    },
    {
      "name": "ApplicationUpdate",
      "path": "/a/update/:id",
      "api": true,
      "get": false
    },
    {
      "name": "Organizations",
      "path": "/o",
      "api": true,
      "get": true
    },
    {
      "name": "OrganizationCreate",
      "path": "/o/create",
      "api": true,
      "get": true
    },
    {
      "name": "Organization",
      "path": "/o/:id",
      "api": true,
      "get": true
    },
    {
      "name": "OrganizationUpdate",
      "path": "/o/update/:id",
      "api": true,
      "get": false
    }
  ]
}
